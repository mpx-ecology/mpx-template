{
  {% if isPlugin %}
  "miniprogramRoot": "miniprogram",
  "pluginRoot": "plugin",
  "compileType": "plugin",
  {% else %}
  "compileType": "miniprogram",
  {% endif %}
  {% if cloudFunc %}
  "cloudfunctionRoot": "functions",
  {% endif %}
  "setting": {
    "newFeature": true,
    "urlCheck": true,
    "es6": false,
    "postcss": false,
    "minified": false,
    "checkSiteMap": false,
    "autoAudits": false
  },
  "appid": "<$ appid $>",
  "projectname": "<$ name $>",
  "description": "<$ description $>",
  "condition": {}
}
